{
  "name": "roguelike-js",
  "version": "0.1.0",
  "description": "A graphical Roguelike in the browser, written in Typescript",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/webpack-env": "^1.18.0",
    "ajv": "^8.12.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "glob": "^8.1.0",
    "glob-import-loader": "^1.2.0",
    "glob-promise": "^6.0.2",
    "jest": "^29.4.3",
    "json-schema-to-typescript": "^11.0.3",
    "rimraf": "^4.1.2",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "scripts": {
    "gen-schema": "tsx scripts/gen-schema.ts",
    "build": "npm run gen-schema && npx webpack",
