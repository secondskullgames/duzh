{
  "name": "duzh",
  "version": "0.1.0",
  "description": "A graphical Roguelike in the browser, written in Typescript",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.8",
    "@types/webpack-env": "^1.18.3",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "ajv": "^8.12.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.52.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "file-loader": "^6.2.0",
    "glob": "^8.1.0",
    "glob-import-loader": "^1.2.0",
    "glob-promise": "^6.0.5",
    "jest": "^29.7.0",
    "json-schema-to-typescript": "^13.1.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "typescript-json-schema": "^0.62.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "gen-schema": "tsx scripts/gen-schema.ts",
    "build": "npm run gen-schema && npx webpack",
    "serve": "npm run build && npx webpack serve",
    "clean": "npx rimraf build && npx rimraf electron/build && npx rimraf src/gen-schema",
    "test": "npm run build && jest",
    "coverage": "npm run build && jest --coverage",
    "electron": "npm run build && cd electron && npm i && npm run make && npx electron .",
    "lint": "npx eslint src/**/*.ts && npx prettier --check src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secondskullgames/duzh.git"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0 <19.0.0"
  },
  "author": "jwbutler",
  "license": "ISC",
  "type": "module"
}
