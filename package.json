{
  "name": "duzh",
  "version": "0.1.0",
  "description": "A graphical Roguelike in the browser, written in Typescript. fnord",
  "author": "jwbutler",
  "license": "ISC",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secondskullgames/duzh.git"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite dev",
    "preview": "vite preview",
    "clean": "rimraf build && rimraf electron/build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "electron": "cd electron && npm i && npm run make && npx electron .",
    "lint": "eslint '{src,scripts}/**/*.ts' --max-warnings 0",
    "lint:fix": "eslint '{src,scripts}/**/*.ts' --fix --max-warnings 0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/pathfinding": "^0.0.9",
    "eslint": "^9.12.0",
    "glob": "^11.0.0",
    "globals": "^15.10.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "tsx": "^4.20.6",
    "typescript": "~5.5.4",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "pathfinding": "^0.4.18",
    "zod": "^4.0.5"
  },
  "overrides": {
    "express": {
      "cookie": "^0.7.1"
    }
  }
}
